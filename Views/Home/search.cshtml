<section id="connectHere" class="section d-none">
	<div class="container">
		<button type="button" class="btn btn-outline-secondary top-left-btn" id="backToSearch"><i class="fa fa-chevron-left"></i> Back</button>
		<div class="row mb-4">
			<div class="col-md-6 mx-auto text-center">
				<div class="badge badge-pill badge-secondary mx-auto">Step 2 of 2</div>
				<h4 class="text-center mt-4">Account URL</h4>
				<div class="mb-4">
					<input type="text" class="form-control form-control-lg" id="" placeholder="https://...">
				</div>
				<h4 class="text-center">Dealer ID</h4>
				<div class="mb-4">
					<input type="text" class="form-control form-control-lg" id="" placeholder="id...">
				</div>
				<h4 class="text-center">Log in</h4>
				<div class="large-input mb-4">
					<input type="text" class="form-control form-control-lg mb-2" id="" placeholder="username...">
					<input type="text" class="form-control form-control-lg" id="" placeholder="password...">
				</div>
				<a href="/service" class="btn btn-primary mt-2">Connect</a>
			</div>
		</div>
	</div>
</section>

@section Scripts {
	<script src="~/js/list.min.js"></script>
	<script>
		var options = {
		  valueNames: [ 'feed-name' ]
		};
		var userList = new List('searchService', options);
	</script>
	<script src="~/js/typeahead.jquery.min.js"></script>
	<script src="~/js/typeahead.services.js"></script>
	
	<script>
		$('#searchService .list a').click( function() {
		    $('#connectHere').removeClass('d-none')
		    $('#searchService').addClass('d-none')
		});
		
		$('#backToSearch').click( function() {
		    $('#connectHere').addClass('d-none')
		    $('#searchService').removeClass('d-none')
		});
	</script>
}